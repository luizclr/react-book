{
  "name": "react-book",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack/webpack.dev --open --hot",
    "lint": "eslint ./",
    "build": "webpack --config webpack/webpack.prod -p",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "fake-backend": "node __fake-backend__/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luizclr/react-book.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/luizclr/react-book/issues"
  },
  "homepage": "https://github.com/luizclr/react-book#readme",
  "dependencies": {
    "@babel/runtime": "^7.6.0",
    "axios": "^0.19.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "coveralls": "^3.0.6",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "json-server": "^0.15.1",
    "node-sass": "^4.12.0",
    "pre-commit": "^1.2.2",
    "pre-push": "^0.1.1",
    "prettier-eslint": "^9.0.0",
    "react-test-renderer": "^16.9.0",
    "sass-loader": "^7.3.1",
    "sinon": "^7.4.2",
    "style-loader": "^1.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.8.0"
  },
  "pre-commit": {
    "run": [
      "lint"
    ]
  },
  "pre-push": {
    "run": [
      "lint",
      "test"
    ]
  },
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "testMatch": [
      "<rootDir>/__tests__/**/*.test.js",
      "<rootDir>/__tests__/*.test.js",
      "<rootDir>/src/**/*.test.js"
    ],
    "setupFiles": [
      "<rootDir>/config/enzyme.config.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
